<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name ="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level ="info">
            <AppenderRef ref="SYSTEM_OUT"/>
        </Root>
    </Loggers>
</Configuration>